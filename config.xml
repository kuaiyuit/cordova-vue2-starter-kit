<?xml version='1.0' encoding='utf-8'?>
<widget id="ky.example.com" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>CordovaVue2</name>
  <description></description>
  <author email="dev@kuaiyuit.com" href="https://github.com/kuaiyuit">kuaiyuit</author>
  <content src="index.html" />
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <engine name="ios" spec="~4.3.1" />
  <engine name="android" spec="~6.2.2" />
  <preference name="android-minSdkVersion" value="16" />
  <plugin name="cordova-plugin-whitelist" spec="1" />
  <plugin name="cordova-plugin-crosswalk-webview" spec="~2.3.0" />
  <plugin name="cordova-plugin-console" spec="~1.0.5" />
  <plugin name="cordova-plugin-device" spec="~1.1.4" />
  <engine name="browser" spec="~4.1.0" />
</widget>
